<?xml version="1.0" encoding="utf-8" ?>
<ircbotconfig>
    <server>localhost</server>
    <channels>
        <channel>#chaskistest</channel>
        <channel>#chaskistest2</channel>
    </channels>
    <port>{%port%}</port>
    <username>chaskisbot</username>
    <nick>chaskisbot</nick>
    <realname>Chaskis Test Bot</realname>
    <password></password>
    <ratelimit>100</ratelimit>
    <watchdogtimeout>30</watchdogtimeout>
    <quitmessage>I'm going down!</quitmessage>
    <admins>
        <admin>adminuser</admin>
    </admins>
    <bridgebots>
        <bridgebot>
            <botname>tg-ritfoss\d*</botname>
            <botregex><![CDATA[^<((\x02(?<bridgeUser>\S+)\x02)|((?<bridgeUser>\S+)))>\s+(?<bridgeMessage>.+)]]></botregex>
        </bridgebot>
    </bridgebots>
</ircbotconfig>
